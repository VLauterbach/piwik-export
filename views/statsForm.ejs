<html>
  <body>
    <form action="stats">
      <select id="month" name="month" /> 
        <% for(var month = 0; month < 12; month++) { %>
          <option <%= new Date().getMonth() == month  ? 'selected' : '' %>><%= months[month] %></option>
        <% } %>
      </select>

      <select id="year" name="year" />
        <% for(var year = 2014; year <= new Date().getFullYear(); year++) { %>
          <option <%= new Date().getFullYear() == year ? 'selected' : '' %>><%= year %></option>
        <% } %> 
      </select> 

      <input type="submit"/>
    </form>
  </body>
</html>